@model IEnumerable<PicturesGallery.Models.GalleryViewModel>
<style>
</style>
<div class="container" style="background-color:aliceblue">
    <div class="row">
        <div class="text-center">
            @foreach (var item in Model)
            {
                <div class="col-md-5 col-sm-10" style="border: 1px solid #ccc;margin:20px; background-color:cadetblue">
                    <img class="img-responsive" src="~/Content/Images/@item.Filename" style="padding:10px" />
                    <div class="text-center">
                        @item.Filename
                    </div>
                </div>
            }
        </div>
    </div>
</div>



